node {
    on_commit to: develop, {
        maven.run(["test"],properties:["skipTests":"true"],"test")
    }
    
    on_pull_request to: master, {
        maven.run(["clean package"],properties:["skipTests":"true"])
        checkmarx.run("Java")
        whitesource.run("maven")
    }
        
    on_merge to: main, from: master, {
        maven.run(["clean package"],properties:["skipTests":"true"])
    }
}
